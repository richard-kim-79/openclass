{
  "name": "@openclass/backend",
  "version": "1.0.0",
  "private": true,
  "description": "OpenClass AI-powered learning platform backend",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed"
  },
  "keywords": [
    "ai",
    "education",
    "learning",
    "vector-search",
    "semantic-search",
    "nodejs",
    "typescript",
    "prisma",
    "postgresql"
  ],
  "dependencies": {
    "@langchain/community": "^0.3.48",
    "@langchain/openai": "^0.5.18",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "chromadb": "^3.0.6",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.29",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "prisma": "^5.7.1",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/socket.io": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
